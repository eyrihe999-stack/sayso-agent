# 运行环境: local | dev | prod（决定加载 config/<env>.yaml）
APP_ENV=local

# Gin 模式: debug | release
GIN_MODE=debug

# 大模型 API Key（会覆盖 yaml 中的 llm.api_key）
LLM_API_KEY=

# 飞书应用（会覆盖 yaml）
FEISHU_APP_ID=
FEISHU_APP_SECRET=
FEISHU_DOMAIN=  # 飞书域名，如 example.feishu.cn，用于生成文档链接

# Slack 机器人 Token（会覆盖 yaml）
SLACK_BOT_TOKEN=
